{
  "name": "lendscape-webpack-build",
  "version": "2.0.0",
  "main": "index.js",
  "author": "lendscape",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "fork-ts-checker-notifier-webpack-plugin": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "preact": "^10.6.2",
    "typescript": "^4.5.2"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=development webpack --env MODE=dev --config ./configs/webpack.dev.js",
    "watch": "npm run build -- --watch",
    "prod": "cross-env NODE_ENV=production webpack --config ./configs/webpack.prod.js",
    "analyse": "npm run prod -- --profile --json | findstr /v /r \"^> $\" > stats.json && webpack-bundle-analyzer ./stats.json",
    "test": "jest",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "prettier": "prettier --write \"./**/*.*\" --ignore-path .prettierignore --config .prettierrc"
  },
